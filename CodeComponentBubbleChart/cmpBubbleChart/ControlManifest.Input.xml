<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="CodeComponent" constructor="cmpBubbleChart" version="1.0.3" display-name-key="cmpBubbleChart" description-key="cmpBubbleChart description" control-type="standard" >
    <!--external-service-usage node declares whether this 3rd party PCF control is using external service or not, if yes, this control will be considered as premium and please also add the external domain it is using.
    If it is not using any external service, please set the enabled="false" and DO NOT add any domain below. The "enabled" will be false by default.
    Example1:
      <external-service-usage enabled="true">
        <domain>www.Microsoft.com</domain>
      </external-service-usage>
    Example2:
      <external-service-usage enabled="false">
      </external-service-usage>
    -->
    <external-service-usage enabled="false">
      <!--UNCOMMENT TO ADD EXTERNAL DOMAINS
      <domain></domain>
      <domain></domain>
      -->
    </external-service-usage>
    <!-- property node identifies a specific, configurable piece of data that the control expects from CDS -->
    
    <property name="chartTitle" display-name-key="Chart Title" description-key="chartTitle" of-type="SingleLine.Text" usage="bound" />
    <property name="chartHeight" display-name-key="Chart Height" description-key="chartHeight" of-type="Whole.None" usage="bound" />
    <property name="chartWidth" display-name-key="Chart Width" description-key="chartWidth" of-type="Whole.None" usage="bound" />

    <property name="titleSize" display-name-key="Title Font Size" description-key="titleSize" of-type="Whole.None" usage="bound"/>
    <property name="chartTextsSize" display-name-key="Texts Font Size" description-key="chartTexts" of-type="Whole.None" usage="bound"/>
    

    <property name="name" display-name-key="Bubble Name" description-key="Bubble Name" of-type="SingleLine.Text" usage="bound" />
    <property name="value" display-name-key="Value" description-key="Value" of-type="SingleLine.Text" usage="bound" />
    <property name="xs" display-name-key="X Axis Position" description-key="X Position" of-type="SingleLine.Text" usage="bound" />
    <property name="ys" display-name-key="Y Axis Position" description-key="Y Position" of-type="SingleLine.Text" usage="bound" />
    <property name="category" display-name-key="Legends" description-key="Legends" of-type="SingleLine.Text" usage="bound" />

    <property name="outlineBubbleColor" display-name-key="Outline Bubble Color" description-key="outlineBubbleColor" of-type="Enum" usage="input" required="false">
      <value name="white" display-name-key="White" default="true">white</value>
      <value name="gray" display-name-key="Gray">darkgray</value>
      <value name="black" display-name-key="Black">Black</value>
    </property>

    <property name="outlineBubbleThickness" display-name-key="Outline Bubble Thickness" description-key="outlineBubbleThickness" of-type="Enum" usage="input" required="false">
      <value name="0" display-name-key="0" default="true">0</value>
      <value name="1" display-name-key="1">1</value>
      <value name="2" display-name-key="2">2</value>
      <value name="3" display-name-key="3">3</value>
      <value name="4" display-name-key="4">4</value>
      <value name="5" display-name-key="5">5</value>
    </property>

    <property name="ColorPalettePreference" display-name-key="Color Palette" description-key="ColorPalettePreference" of-type="Enum" usage="input" required="false">
      <value name="colorSet1" display-name-key="Color Set 1" default="true">colorSet1</value>
      <value name="colorSet2" display-name-key="Color Set 2">colorSet2</value>
      <value name="colorSet3" display-name-key="Color Set 3">colorSet3</value>
      <value name="colorSet4" display-name-key="Color Set 4">colorSet4</value>
      <value name="colorSet5" display-name-key="Color Set 5">colorSet5</value>
    </property>

    <!--
      Property node's of-type attribute can be of-type-group attribute.
      Example:
      <type-group name="numbers">
        <type>Whole.None</type>
        <type>Currency</type>
        <type>FP</type>
        <type>Decimal</type>
      </type-group>
      <property name="sampleProperty" display-name-key="Property_Display_Key" description-key="Property_Desc_Key" of-type-group="numbers" usage="bound" required="true" />
    -->
    <resources>
      <code path="index.ts" order="1"/>
      <!-- UNCOMMENT TO ADD MORE RESOURCES
      <css path="css/cmpBubbleChart.css" order="1" />
      <resx path="strings/cmpBubbleChart.1033.resx" version="1.0.0" />
      -->
    </resources>
    <!-- UNCOMMENT TO ENABLE THE SPECIFIED API
    <feature-usage>
      <uses-feature name="Device.captureAudio" required="true" />
      <uses-feature name="Device.captureImage" required="true" />
      <uses-feature name="Device.captureVideo" required="true" />
      <uses-feature name="Device.getBarcodeValue" required="true" />
      <uses-feature name="Device.getCurrentPosition" required="true" />
      <uses-feature name="Device.pickFile" required="true" />
      <uses-feature name="Utility" required="true" />
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
    -->
  </control>
</manifest>
